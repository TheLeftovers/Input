<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="project.pkg56">
  <class lazy="false" name="project.pkg56.Person" table="t_person">
    <id column="ID" name="id">
      <generator class="increment"/>
    </id>
    <property column="FIRST_NAME" name="firstName"/>
    <property column="LAST_NAME" name="lastName"/>
    <property column="HOURS" name="Hours"/>
  </class>
</hibernate-mapping>
