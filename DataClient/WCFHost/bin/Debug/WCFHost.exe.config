<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.1" />
    </startup>
    <system.serviceModel>
        <behaviors>
            <serviceBehaviors>
                <behavior name="getterBehavior">
                    <serviceMetadata httpGetEnabled="true" httpGetBinding="webHttpBinding"
                        httpGetBindingConfiguration="" />
                </behavior>
            </serviceBehaviors>
        </behaviors>
        <services>
            <service behaviorConfiguration="getterBehavior" name="WCF.Service1">
                <endpoint address="getter" binding="basicHttpBinding" bindingConfiguration=""
                    contract="WCF.IService1" />
              <endpoint address="getter" binding="netTcpBinding" bindingConfiguration=""
                        contract="WCF.IService1" />
              <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange"/>
              <endpoint address="mex" binding="mexTcpBinding" contract="IMetadataExchange"/>
              <host>
                    <baseAddresses>
                        <add baseAddress="http://localhost:6789" />
                      <add baseAddress="net.tcp://localhost:7890" />
                    </baseAddresses>
                </host>
            </service>
        </services>
    </system.serviceModel>
</configuration>